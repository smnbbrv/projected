{
  "name": "projected",
  "version": "0.1.0",
  "description": "Collections of objects that rely on remote data sources. Hides the complexity of fetching and caching data from a remote source",
  "type": "module",
  "author": "smnbbrv",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/smnbbrv/projected.git"
  },
  "bugs": {
    "url": "https://github.com/smnbbrv/projected/issues"
  },
  "homepage": "https://github.com/smnbbrv/projected#readme",
  "scripts": {
    "start": "vite build --watch",
    "build": "vite build",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "clear": "rm -rf dist",
    "patch": "node ci/patcher.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "typecheck": "tsc --noEmit",
    "validate": "run-p lint typecheck coverage",
    "release": "semantic-release"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/exec": "^7.1.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@types/node": "^25.2.0",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "@vitest/coverage-istanbul": "^4.0.18",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.5",
    "npm-run-all": "^4.1.5",
    "semantic-release": "^25.0.3",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "vite": "^7.3.1",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.18"
  },
  "files": [
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  }
}
